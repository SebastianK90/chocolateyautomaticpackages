        <licenseUrl>https://www.google.com/intl/en/chrome/browser/privacy/eula_text.html</licenseUrl>
		<packageSourceUrl>https://github.com/SebastianK90/chocolateyautomaticpackages/tree/master/choco-chrome-ext</packageSourceUrl>
